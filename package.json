{
  "name": "smart-nft",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack  --mode development -w",
    "build-webpack": "webpack  --mode production ",
    "watch-sass-frontend": "node-sass frontend/src/main.scss frontend/assets/css/style.css -w",
    "compile-sass-frontend": "node-sass frontend/src/main.scss frontend/assets/css/style.comp.css",
    "concat-css-frontend": "concat -o frontend/assets/css/style.concat.css frontend/assets/css/style.comp.css",
    "prefix-css-frontend": "postcss --use autoprefixer -b 'last 10 versions' frontend/assets/css/style.concat.css -o frontend/assets/css/style.prefix.css",
    "compress-css-frontend": "node-sass frontend/assets/css/style.prefix.css frontend/assets/css/style.css --output-style compressed",
    "watch-sass-backend": "node-sass backend/src/main.scss backend/assets/css/style.css -w",
    "compile-sass-backend": "node-sass backend/src/main.scss backend/assets/css/style.comp.css",
    "concat-css-backend": "concat -o backend/assets/css/style.concat.css backend/assets/css/style.comp.css",
    "prefix-css-backend": "postcss --use autoprefixer -b 'last 10 versions' backend/assets/css/style.concat.css -o backend/assets/css/style.prefix.css",
    "compress-css-backend": "node-sass backend/assets/css/style.prefix.css backend/assets/css/style.css --output-style compressed",
    "build-css-frontend": "npm-run-all compile-sass-frontend concat-css-frontend prefix-css-frontend compress-css-frontend",
    "build-css-backend": "npm-run-all compile-sass-backend concat-css-backend prefix-css-backend compress-css-backend",
    "build-css": "npm-run-all build-css-frontend build-css-backend",
    "start-hardhat": "npx hardhat node",
    "deploy-contract-locally": "npx hardhat run scripts/deploy.js --network localhost",
    "generate-deploy-code": "node scripts/build.js",
    "start": "npm-run-all --parallel  webpack watch-sass-frontend watch-sass-backend",
    "build": "npm-run-all -s build-webpack build-css generate-deploy-code"
  },
  "dependencies": {
    "@react-three/drei": "^9.40.0",
    "@react-three/fiber": "^8.9.1",
    "@walletconnect/sign-client": "^2.2.1",
    "@web3modal/standalone": "^2.0.0-rc.2",
    "ethers": "^5.7.1",
    "ipfs-http-client": "^57.0.3",
    "react": "^18.2.0",
    "react-3d-viewer": "^1.0.12",
    "react-dom": "^18.2.0",
    "react-infinite-scroller": "^1.2.6",
    "three": "^0.146.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tophive/smart-nft.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tophive/smart-nft/issues"
  },
  "homepage": "https://github.com/tophive/smart-nft#readme",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@openzeppelin/contracts": "^4.8.0",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "concat": "^1.0.3",
    "css-loader": "^6.7.1",
    "hardhat": "^2.10.1",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0"
  }
}
